{
  "expo": {
    "name": "picturesCollection",
    "slug": "picturesCollection",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "preview": {
        "android": {
          "buildType": "apk"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {},
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA"
      ],
      "package": "com.picturesCollection.com",
      "versionCode": 1
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take pictures.",
        "NSPhotoLibraryUsageDescription": "Allow picture collection to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow picture collection to save photos."
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.maoapp.picturesCollection"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "allow picture collection to take photos."
        }
      ],
      [
        "expo-media-library",
        {
          "savePhotosPermission": "Allow picture collection to save photos.",
          "photosPermission": "Allow picture collection to access your photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "af8e1dac-34f8-47a9-9d3d-e7df3b43cb1a"
      }
    }
  }
}
